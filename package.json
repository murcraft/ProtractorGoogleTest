{
  "name": "protractorgoogletest",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "preupdate-webdriver": "npm install",
    "webdriver-manager-clean": "webdriver-manager clean",
    "update-webdriver": "webdriver-manager update --gecko=false --chrome=false --versions.standalone=3.14.0",
    "update-chrome": "webdriver-manager update --gecko=false --versions.chrome=2.46 --versions.standalone=3.4.0",
    "update-gecko": "webdriver-manager update --chrome=false --versions.standalone=3.14.0",
    "update-ie": "webdriver-manager update --gecko=false --chrome=false --versions.ie=3.14.0 --versions.standalone=3.14.0",
    "start-webdriver": "webdriver-manager start --versions.standalone=3.14.0 --chrome=false --detach",
    "start-ie-webdriver": "webdriver-manager start --versions.standalone=3.14.0 --chrome=false --gecko=false --versions.ie=3.14.0 --detach",
    "test-chrome": "npm run update-chrome",
    "test-gecko": "npm run update-gecko && npm run start-webdriver",
    "test-safari": "npm run update-webdriver",
    "test-ie": "npm run update-ie && npm run start-ie-webdriver && npm run test",
    "test-clean": "npm run webdriver-manager-clean",
    "test": "node conf-flake.js"
  },
  "devDependencies": {
    "aws-sdk": "^2.436.0",
    "browserstack-local": "^1.4.0",
    "grunt": "^1.0.4",
    "grunt-protractor-runner": "^5.0.0",
    "gulp": "^4.0.0",
    "gulp-notify": "^3.2.0",
    "gulp-protractor": "^4.1.1",
    "jasmine-allure-reporter": "^1.0.2",
    "jasmine-data-provider": "^2.2.0",
    "log4js": "^3.0.6",
    "protractor": "^5.4.2",
    "protractor-flake": "3.3.0",
    "protractor-stop-describe-on-failure": "^1.0.3",
    "request-promise": "^4.2.2",
    "slack-node": "^0.1.8",
    "shelljs": "^0.8.3"
  }
}
