{
  "name": "protractorgoogletest",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update --gecko=false --versions.standalone=3.14.0",
    "start-webdriver": "webdriver-manager start --detach",
    "update-chrome": "webdriver-manager update --gecko=false --versions.chrome=2.45 --versions.standalone=3.4.0",
    "update-gecko": "webdriver-manager update --chrome=false",
    "pretest-webdriver": "npm run update-webdriver",
    "pretest-chrome": "npm run update-chrome",
    "pretest-gecko": "npm run update-gecko",
    "test-chrome": "npm run preupdate-webdriver && npm run pretest-chrome && node conf-flake.js",
    "test-gecko": "npm run preupdate-webdriver && npm run pretest-gecko && npm run start-webdriver && node conf-flake.js -- browser=firefox suite=suite1",
    "test": "npm run preupdate-webdriver && npm run pretest-webdriver && node conf-flake.js"
  },
  "devDependencies": {
    "aws-sdk": "^2.436.0",
    "firefox-profile": "^1.2.0",
    "grunt": "^1.0.4",
    "grunt-protractor-runner": "^5.0.0",
    "gulp": "^4.0.0",
    "gulp-notify": "^3.2.0",
    "gulp-protractor": "^4.1.1",
    "jasmine-allure-reporter": "^1.0.2",
    "jasmine-data-provider": "^2.2.0",
    "log4js": "^3.0.6",
    "protractor": "^5.4.2",
    "protractor-flake": "3.3.0",
    "protractor-stop-describe-on-failure": "^1.0.3",
    "request-promise": "^4.2.2",
    "slack-node": "^0.1.8"
  }
}
